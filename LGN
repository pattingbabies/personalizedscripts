-- Click Function
local VirtualInputManager = game:GetService("VirtualInputManager")
function Click(A_1)
    local X = A_1.AbsolutePosition.X
    local Y = A_1.AbsolutePosition.Y
    VirtualInputManager:SendMouseButtonEvent(X + X/2, Y + 50, 0, true, A_1, 1)
    VirtualInputManager:SendMouseButtonEvent(X + X/2, Y + 50, 0, false, A_1, 1)
end

Click(game.Players.LocalPlayer.PlayerGui.Example.Frame.Redeem)
