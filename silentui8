local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Bronze Reforged", "BloodTheme")
local MainTab = Window:NewTab("Main")
local WelpSection = MainTab:NewSection("Main Functions")
WelpSection:NewButton("Heal Pokemons", "Heals all your pokemons", function()
    local args = {
        [1] = "XEDlnUh2SxuwTLuwl2Em4Q",
        [2] = "PDS",
        [3] = "getPartyPokeBalls"
    }
    
    game:GetService("ReplicatedStorage").GET:InvokeServer(unpack(args))
    wait()
end)

local MainTab = Window:NewTab("Route 1")
local WelpSection1 = MainTab:NewSection("Functions")

WelpSection1:NewButton("Gate TP", "Teleports you to Gate for you to move on.", function()
    local object = game:GetService("Workspace").chunk1.Gate.Darkness.Part
    local plrchar = game:GetService("Players").LocalPlayer.Character
    
    plrchar:MoveTo(object.Position)

end)


WelpSection1:NewButton("TP To Items", "Teleports you to items ingame.", function()
    local object = game:GetService("Workspace").chunk1["#Item"].Hinge
    local plrchar = game:GetService("Players").LocalPlayer.Character
    
    plrchar:MoveTo(object.Position)
end)

