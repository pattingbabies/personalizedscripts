  function strength()
    spawn(function()
        while getgenv().autostrength == true do
            
local args = {
    [1] = "Strength"
}

game:GetService("ReplicatedStorage").Events.Train:FireServer(unpack(args))
            wait()
        end
    end)
    end

    function endurance()
        spawn(function()
            while getgenv().autoendurance == true do
                local args = {
                    [1] = "Endurance"
                }
                
                game:GetService("ReplicatedStorage").Events.Train:FireServer(unpack(args))
                wait()
            end
        end)
        end

        function psychic()
            spawn(function()
                while getgenv().autopsychic == true do
                    local args = {
                        [1] = "Psychic"
                    }
                    
                    game:GetService("ReplicatedStorage").Events.Train:FireServer(unpack(args))
                    wait()
                end
            end)
            end





