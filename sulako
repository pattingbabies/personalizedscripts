  function strength()
    spawn(function()
        while getgenv().autostrength == true do
            
local args = {
    [1] = "Strength"
}

game:GetService("ReplicatedStorage").Events.Train:FireServer(unpack(args))
            wait()
        end
    end)
    end

    function endurance()
        spawn(function()
            while getgenv().autoendurance == true do
                local args = {
                    [1] = "Endurance"
                }
                
                game:GetService("ReplicatedStorage").Events.Train:FireServer(unpack(args))
                wait()
            end
        end)
        end

        function psychic()
            spawn(function()
                while getgenv().autopsychic == true do
                    local args = {
                        [1] = "Psychic"
                    }
                    
                    game:GetService("ReplicatedStorage").Events.Train:FireServer(unpack(args))
                    wait()
                end
            end)
            end

            function pks()
                spawn(function()
                    while getgenv().upgstr == true do
                            local args = {
                                [1] = "StrengthMultiplier"
                            }
                            
                            game:GetService("ReplicatedStorage").Functions.Multiplier:InvokeServer(unpack(args))
                        wait()
                    end
                end)
                end

                function lps()
                    spawn(function()
                        while getgenv().upgend == true do
                            local args = {
                                [1] = "EnduranceMultiplier"
                            }
                            
                            game:GetService("ReplicatedStorage").Functions.Multiplier:InvokeServer(unpack(args))
                            wait()
                        end
                    end)
                    end

                    function rpk()
                        spawn(function()
                            while getgenv().upgpsy == true do
                                local args = {
                                    [1] = "PsychicMultiplier"
                                }
                                
                                game:GetService("ReplicatedStorage").Functions.Multiplier:InvokeServer(unpack(args))
                                wait()
                            end
                        end)
                        end

                        function rkr()
                            spawn(function()
                                while getgenv().upgspe == true do
                                    local args = {
                                        [1] = "SpeedMultiplier"
                                    }
                                    
                                    game:GetService("ReplicatedStorage").Functions.Multiplier:InvokeServer(unpack(args))
                                    wait()
                                end
                            end)
                            end



